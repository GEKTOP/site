let a = prompt();
let b = prompt();
let znak = prompt();
switch (znak) {
	case '1':
		result = a + b
		alert(result);
		break;
	case '2':
		result = a - b 
		alert(result);
		break;
	case '3':
		result = a * b 
		alert(result);
		break;
	case '4':
		result = a / b 
		alert(result);
		break;
	default:
		result = 'ошибка';
}
